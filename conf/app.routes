# microservice specific routes

GET        /reports                                     @uk.gov.hmrc.leakdetectionfrontend.controllers.ReportsController.redirectToRepositories
GET        /reports/repositories                        @uk.gov.hmrc.leakdetectionfrontend.controllers.ReportsController.repositories
GET        /reports/repositories/:repository            @uk.gov.hmrc.leakdetectionfrontend.controllers.ReportsController.reportsForRepository(repository: String)
GET        /reports/:id                                 @uk.gov.hmrc.leakdetectionfrontend.controllers.ReportsController.showReport(id: String)

GET        /assets/*file                                controllers.Assets.at(path="/public", file)
